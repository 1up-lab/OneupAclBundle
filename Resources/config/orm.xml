<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="oneup_acl.doctrine_subscriber" class="Oneup\AclBundle\EventListener\DoctrineSubscriber">
            <argument type="service" id="oneup_acl.driver_chain" />
            <argument type="service" id="oneup_acl.manager" />
            <argument>%oneup_acl.remove_orphans%</argument>
            <tag name="doctrine.event_subscriber" />
        </service>
    </services>
</container>
